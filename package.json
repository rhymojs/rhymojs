{
  "name": "@rhymo/core",
  "version": "1.4.1",
  "description": "Avoid writing repetitive React code.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "test": "jest --verbose",
    "test:watch": "jest --verbose",
    "lint": "yarn eslint --ext .ts,.tsx src/",
    "lint:fix": "yarn lint --fix",
    "prettify": "prettier --write src/**/**/*.ts src/**/**/*.tsx",
    "prepare": "husky install",
    "release": "standard-version",
    "release:major": "standard-version --release-as major && yarn build && npm publish",
    "release:minor": "standard-version --release-as minor && yarn build && npm publish",
    "release:patch": "standard-version --release-as patch && yarn build && npm publish",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhymojs/rhymojs.git"
  },
  "keywords": [
    "rhymojs"
  ],
  "author": "Xeptao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhymojs/rhymojs/issues"
  },
  "homepage": "https://github.com/rhymojs/rhymojs",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "react-test-renderer": "^17.0.2",
    "standard-version": "^9.3.0",
    "ts-jest": "^27.0.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "src/**/*.{ts, tsx}": [
      "yarn lint:fix",
      "yarn prettify",
      "yarn test",
      "git add . -A"
    ]
  }
}
